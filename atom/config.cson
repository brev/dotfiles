"*":
  "atom-ide-ui":
    use:
      "atom-ide-diagnostics-ui": "never"
  "atom-ternjs":
    inlineFnCompletionDocumentation: true
  core:
    audioBeep: false
    disabledPackages: [
      "styleguide"
      "language-clojure"
      "language-csharp"
      "language-go"
      "language-java"
      "language-less"
      "language-objective-c"
      "language-perl"
      "language-php"
      "language-ruby"
      "language-ruby-on-rails"
      "rename-tabs"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "node_modules"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "solarized-light-syntax"
    ]
  editor:
    fontFamily: "Menlo"
    fontSize: 16
    invisibles: {}
    lineHeight: 1.36
    scrollSensitivity: 80
    showIndentGuide: true
    softWrapped: false
  "exception-reporting":
    userId: "88838393-5dc9-aff0-337c-cfa8b8b7370a"
  "file-header":
    numOfEmptyLinesAfterNewHeader: 1
  "highlight-line": {}
  linter: {}
  "linter-stylelint":
    enableHtmlLinting: true
  "linter-ui-default":
    alwaysTakeMinimumSpace: true
    panelHeight: 188
    showPanel: true
  metrics:
    userId: "b41e910c48c82373ed1dc8738d1b95370494448d"
  minimap:
    plugins:
      bookmarks: true
      bookmarksDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
      selection: true
      selectionDecorationsZIndex: 0
  "one-light-ui": {}
  "platformio-ide-terminal":
    core:
      scrollback: 100000
    style:
      fontFamily: "Meslo LG M for Powerline"
      fontSize: "15"
      theme: "solarized-light"
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.html.mustache"
      "source.js.jsx"
      "source.json"
      "source.gfm"
    ]
  "vim-mode-plus": {}
  "webbox-color": {}
  welcome:
    showOnStartup: false
  whitespace:
    ensureSingleTrailingNewline: true
    ignoreWhitespaceOnCurrentLine: false
    removeTrailingWhitespace: true
".gfm.source":
  editor:
    softWrap: false
