### exports

export CLICOLOR=1
unset LSCOLORS
export PATH=$PATH:/usr/local/share/npm/bin:~/bin

### aliases

alias grep='grep --color=auto'

### other

# Git bash completion
if [ -f `/usr/local/bin/brew --prefix`/etc/bash_completion ]; then
	. `/usr/local/bin/brew --prefix`/etc/bash_completion
fi

### prompt

export PS1='[\h \w$(__git_ps1 " (%s)")]\$ '